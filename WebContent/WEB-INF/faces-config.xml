<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<description>This Bean is for handling login sessions</description>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>org.bsanalytics.login.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>progressBarBean</managed-bean-name>
		<managed-bean-class>org.bsanalytics.progressbar.ProgressBarBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>clientLoadDataCall</managed-bean-name>
		<managed-bean-class>org.bsanalytics.client.loaddata.ClientLoadDataCall</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>fileChooserForLoading</managed-bean-name>
		<managed-bean-class>org.bsanalytics.dashboard.FileChooserForLoadingData</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>viewTableBackingBean</managed-bean-name>
		<managed-bean-class>org.bsanalytics.client.viewdata.ViewTableBackingBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dynamicDataModel</managed-bean-name>
		<managed-bean-class>org.bsanalytics.apis.viewdata.DynamicDataModel</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actionListenerViewLoadData</managed-bean-name>
		<managed-bean-class>org.bsanalytics.ActionListeners.ActionListenerViewLoadData</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dataScrollerBeanForViewData</managed-bean-name>
		<managed-bean-class>org.bsanalytics.client.viewdata.DataScrollerBeanForViewData</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dynamicPaginationLoadingViewModel</managed-bean-name>
		<managed-bean-class>org.bsanalytics.client.viewdata.DynamicPaginationLoadingViewModel</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pagination</managed-bean-name>
		<managed-bean-class>org.bsanalytics.client.viewdata.Pagination</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>createDeleteTableBean</managed-bean-name>
		<managed-bean-class>org.bsanalytics.client.loaddata.CreateDeleteTableBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loadDataBean</managed-bean-name>
		<managed-bean-class>org.bsanalytics.client.loaddata.LoadDataBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>login/login</display-name>
		<from-view-id>/login/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>login_in</from-outcome>
			<to-view-id>/dashboard/dashboard_template.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

</faces-config>
